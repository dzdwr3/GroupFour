package cn.group4.page;

import org.openqa.selenium.WebDriver;

import cn.group4.utils.BasePage;
import cn.group4.utils.Locator;

public class Product_Doc_Add extends BasePage{

	Locator iptDocTitle = new Locator("iptDocTitle");
	Locator btnSaveDoc = new Locator("btnSaveDoc");
	
	public Product_Doc_Add(WebDriver driver) throws Exception {
		super(driver);
	}
	
	public void  typeOniptDocTitle(String value) throws Exception
	{
		type(iptDocTitle, value);
	}
	
	public void  clickOnbtnSaveDoc() throws Exception
	{
		click(btnSaveDoc);
	}
	
}
